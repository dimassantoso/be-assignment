{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "JSON Schema for get all",
  "type": "object",
  "properties": {
    "page": {
      "type": "number",
      "default": 1,
      "minimum": 0
    },
    "limit": {
      "type": "number",
      "default": 10,
      "minimum": 1
    },
    "order_by": {
      "type": "string",
      "enum": [
        "id",
        "name",
        "created_at",
        "updated_at",
        "email"
      ]
    },
    "sort": {
      "type": "string",
      "enum": [
        "asc",
        "desc",
        "ASC",
        "DESC"
      ]
    },
    "search": {
      "type": "string"
    }
  },
  "additionalProperties": true
}
