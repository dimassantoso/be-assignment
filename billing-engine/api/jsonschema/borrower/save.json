{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "JSON Schema for save",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "email": {
      "type": "string",
      "minLength": 1,
      "format": "email"
    }
  },
  "required": [
    "name",
    "email"
  ],
  "additionalProperties": false
}
